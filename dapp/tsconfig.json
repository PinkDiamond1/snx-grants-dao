{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node",
    "lib": ["dom", "es2017"],
    "jsx": "preserve",
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
